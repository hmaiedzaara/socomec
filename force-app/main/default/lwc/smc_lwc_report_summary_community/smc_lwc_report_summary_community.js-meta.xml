<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>48.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>Report Summarize Community</masterLabel>
    <targets>
        <target>lightningCommunity__Page</target>
        <target>lightningCommunity__Default</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightningCommunity__Default">
            <!-- <property name="picto" type="String" label="Picto" description="Choose an icon in SLDS library." default="Picto"/> -->
            <property name="titleIcon" type="String" label="Icon Title" description="A CSS class as [fa fa-tickets]" default="fa fa-ticket fa-2x"/>
            <property name="title" type="String" label="Title" default="TITLE"/>
            <property name="reportName" type="String" label="Report Name" description="Report label, not APIName" default="IOT Demo - Cases"/>
            <property name="bodyLabel" type="String" label="Text body" description="Describe the number displayed" default="Open Cases"/>
            <property name="buttonLabel" type="String" label="Button label" description="Name of the action triggered with the button" default="New Case"/>
            <property name="urlButton" type="String" label="Button url" description="Url link to the button"  default="/[the-community-name]/s/create-new-standard-case"/>
            <property name="footerLabel" type="String" label="Footer label" description="Label of the footer link" default="View All My Cases"/>
            <property name="footerUrl" type="String" label="Link to related list" description="Url link to the footer" default="/[the-community-name]/s/case/Case/00B1o000005RjHMEA0"/>
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>